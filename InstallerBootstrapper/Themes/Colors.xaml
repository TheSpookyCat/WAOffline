<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Surface">#CC1C1F2A</Color>
    <Color x:Key="Chrome">#CC111420</Color>
    <Color x:Key="Accent">#6CC7FF</Color>
    <Color x:Key="Success">#63D471</Color>
    <Color x:Key="Danger">#E25C56</Color>
    <Color x:Key="Outline">#2E3344</Color>
    <Color x:Key="OnSurfaceColor">#F4F6FB</Color>
    <Color x:Key="OnSurfaceVariantColor">#A7AEC2</Color>
    <Color x:Key="OnChromeColor">#E8EAF6</Color>

    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource Surface}" />
    <SolidColorBrush x:Key="ChromeBrush" Color="{StaticResource Chrome}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}" />
    <SolidColorBrush x:Key="SuccessBrush" Color="{StaticResource Success}" />
    <SolidColorBrush x:Key="DangerBrush" Color="{StaticResource Danger}" />
    <SolidColorBrush x:Key="OutlineBrush" Color="{StaticResource Outline}" />
    <SolidColorBrush x:Key="OnSurface" Color="{StaticResource OnSurfaceColor}" />
    <SolidColorBrush x:Key="OnSurfaceVariant" Color="{StaticResource OnSurfaceVariantColor}" />
    <SolidColorBrush x:Key="OnChrome" Color="{StaticResource OnChromeColor}" />
    <SolidColorBrush x:Key="CardBrush" Color="#CC23283B" />
</ResourceDictionary>
